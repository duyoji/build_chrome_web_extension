{
  "manifest_version": 2,

  "name": "Quick Song Player",
  "description": "This extension will implement bookmark features.",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon2.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [],
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["popup.js"]
  }]
}
